package hibernate.demo.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="libros")
public class autor {

	@Id
	@Column(name="idLibro")
	private int idLibro;
	
	@Column(name="nombre")
    private String nombre;
	
	@Column(name="sponsor")
    private String sponsor;
		
		public autor(){
			
	
		
	}

}